{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },

  "B2": {
    "KeyId": "YOUR_BACKBLAZE_KEY_ID",
    "ApplicationKey": "YOUR_BACKBLAZE_APPLICATION_KEY",
    "BucketName": "YOUR_BUCKET_NAME",
    "Endpoint": "https://s3.your-region.backblazeb2.com"
  },

  "Bloom": {
    "BloomFilterDirectory": "Path/To/Your/BloomFilter/Directory",
    "BloomDataFileName": "bloomFilterData",
    "BloomTempFileName": "bloomFilterTemp",
    "expectedElements": 10000000,
    "errorRate": 0.01
  },

  "BloomRebuild": {
    "RebuildTime": "03:00:00"
  },

  "Jwt": {
    "Key": "YOUR_SUPER_SECRET_SECURITY_KEY_MIN_32_CHARS",
    "Time": "30",
    "Issuer": "TeachHub.Api",
    "Audience": "TeachHub.Api"
  },

  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=YourDatabaseName;Username=postgres;Password=YourPassword;Include Error Detail=true"
  },

  "AllowedHosts": "*"
}